> install.packages("tibble")
> library(tibble)
> library(tidyverse)
> # Function to generate dummy mRNA sequence
> generate_sequence <- function(length){
+     bases <- c("A", "C", "G", "U")
+     paste0(sample(bases, length, replace = TRUE), collapse = "")
+     }
> # Parameter for the data
> num_sequences <- 10
> sequence_length <- 36
> # Generate the data
> dummy_data <- tibble(
+     sequence_id = paste0("seq", 1:num_sequences),
+     sequence = map_chr(1:num_sequences, ~generate_sequence(sequence_length))
+ )
> # Display the data
> print(dummy_data)
# A tibble: 10 × 2
   sequence_id sequence                       
   <chr>       <chr>                          
 1 seq1        ACCCGUGGUACGAGUCAGCGCGCUGACAAU…
 2 seq2        AACCCAGCAUUGUGCACAGCAAUUAACACU…
